<%= form_with url: admin_search_path , method: :get, class: "input-group" do |f| %>
  <%= f.text_field :content, class: "form-control", placeholder: "Search content", style: "margin-right: 5px;" %>
  <div class="input-group-append" style="margin-right: 5px;">
  <%= f.select :model, options_for_select({ "User" => "user", "投稿タイトル" => "article" }), class: "custom-select"%>
  </div>
  <%= f.submit 'Search',class: "btn btn-outline-success my-2 my-sm-0" %>
<% end %>